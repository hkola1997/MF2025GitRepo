        <fullName>smartlists__Check_Hyperlink_To_Detail</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL (smartlists__Smart_List_Definition__r.smartlists__Data_Source_Type__c, &apos;Apex Data Source&apos; ) &amp;&amp; ISPICKVAL(smartlists__Display_Type__c, &apos;HYPERLINK_DETAIL&apos;) &amp;&amp; ISBLANK(smartlists__Hyperlink_to_Detail_Id_Field__c)</errorConditionFormula>
        <errorDisplayField>smartlists__Hyperlink_to_Detail_Id_Field__c</errorDisplayField>
        <errorMessage>Hyperlink to Detail Id Field is required when Display Type is Hyperlink to Detail for an Apex Data Source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Lookup_Subtitle</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS( smartlists__Lookup_Subtitle_Field__c , &apos;.&apos;)</errorConditionFormula>
        <errorDisplayField>smartlists__Lookup_Subtitle_Field__c</errorDisplayField>
        <errorMessage>Lookup Subtitle cannot be a related field such as Account.Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Lookup_on_Related_Field</fullName>
        <active>true</active>
        <errorConditionFormula>smartlists__Lookup_in_Quick_Filters__c &amp;&amp; smartlists__Field_Name__c != &apos;Owner&apos; &amp;&amp; (LEN( smartlists__Field_Name__c ) - LEN ( SUBSTITUTE ( smartlists__Field_Name__c , &apos;.&apos;, &apos;&apos;))) &lt;&gt; 1</errorConditionFormula>
        <errorDisplayField>smartlists__Lookup_in_Quick_Filters__c</errorDisplayField>
        <errorMessage>Lookup in Quick Filters is only available for related parent fields such as Account.Name or for Owner. It is not available for base fields such as Name or related grand parent fields such as Account.Parent.Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Number_of_Columns</fullName>
        <active>true</active>
        <errorConditionFormula>smartlists__Number_of_columns__c &lt; 1 ||  smartlists__Number_of_columns__c &gt; 3</errorConditionFormula>
        <errorDisplayField>smartlists__Number_of_columns__c</errorDisplayField>
        <errorMessage>Number of Columns must be between 1 and 3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_URL_Value</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( smartlists__URL_Value__c ) &amp;&amp;  ISPICKVAL( smartlists__Display_Type__c , &apos;URL_LABEL&apos;)</errorConditionFormula>
        <errorDisplayField>smartlists__URL_Value__c</errorDisplayField>
        <errorMessage>URL Value is required for Display Type &apos;URL with Label&apos;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>