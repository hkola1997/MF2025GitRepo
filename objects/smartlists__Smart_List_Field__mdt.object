        <errorDisplayField>smartlists__Badge_Style_Field__c</errorDisplayField>
        <errorMessage>Badge Style Field is required if the field is displayed as a Badge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Display_Position</fullName>
        <active>true</active>
        <errorConditionFormula>smartlists__List_Field__c == true &amp;&amp; ISBLANK(smartlists__Display_Position__c)</errorConditionFormula>
        <errorDisplayField>smartlists__Display_Position__c</errorDisplayField>
        <errorMessage>Display Position is required for List fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Display_Position_QuickFilters</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( smartlists__Quick_Filters_Field__c, &apos;Yes if Filterable&apos; ) &amp;&amp; ISBLANK( smartlists__Display_Position_in_Quick_Filters__c )</errorConditionFormula>
        <errorDisplayField>smartlists__Display_Position_in_Quick_Filters__c</errorDisplayField>
        <errorMessage>Display Position is required for Filters Panel fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Display_Type_Picklist</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Smart_List_Definition__r.smartlists__Data_Source_Type__c, &apos;Apex Data Source&apos;) &amp;&amp; ISPICKVAL(smartlists__Display_Type__c, &apos;Picklist&apos;)</errorConditionFormula>
        <errorDisplayField>smartlists__Display_Type__c</errorDisplayField>
        <errorMessage>Picklist is not supported for Apex Data Source lists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Display_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Smart_List_Definition__r.smartlists__Data_Source_Type__c, &apos;Apex Data Source&apos;) &amp;&amp; ISPICKVAL(smartlists__Display_Type__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>smartlists__Display_Type__c</errorDisplayField>
        <errorMessage>Display Type is required for Apex Data Source lists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Editable_in_List_Related_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>smartlists__Editable_in_List__c &amp;&amp;(LEN( smartlists__Field_Name__c ) - LEN ( SUBSTITUTE ( smartlists__Field_Name__c , &apos;.&apos;, &apos;&apos;))) &gt; 1</errorConditionFormula>
        <errorDisplayField>smartlists__Editable_in_List__c</errorDisplayField>
        <errorMessage>Fields can only be edited if they are on the base object or are related to a parent record of the base object.
Example on Account: Parent.Parent.Name is not editable because it is on the parent account  of the parent account. Parent.Name is editable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Field_Label</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Smart_List_Definition__r.smartlists__Data_Source_Type__c, &apos;Apex Data Source&apos;) &amp;&amp; ISBLANK(smartlists__Field_Label__c)</errorConditionFormula>
        <errorDisplayField>smartlists__Field_Label__c</errorDisplayField>
        <errorMessage>Field Label is required for Apex Data Source lists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_File_Preview</fullName>
