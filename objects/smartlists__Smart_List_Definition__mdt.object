    <label>Smart List Definition</label>
    <pluralLabel>Smart List Definitions</pluralLabel>
    <validationRules>
        <fullName>smartlists__Check_Data_Provider</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source&quot;) &amp;&amp;
ISBLANK(smartlists__Data_Provider_Class__c)</errorConditionFormula>
        <errorDisplayField>smartlists__Data_Provider_Class__c</errorDisplayField>
        <errorMessage>Data Provider Class is required for type &apos;	Apex Data Source&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Default_Scope</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(smartlists__SOQL_Default_Scope__c, &apos;everything&apos;) &amp;&amp; ! smartlists__SOQL_Scope_All__c) || (ISPICKVAL(smartlists__SOQL_Default_Scope__c, &apos;mine&apos;) &amp;&amp; ! smartlists__SOQL_Scope_My__c) || (ISPICKVAL(smartlists__SOQL_Default_Scope__c, &apos;team&apos;) &amp;&amp; ! smartlists__SOQL_Scope_My_Team__c) || (ISPICKVAL(smartlists__SOQL_Default_Scope__c, &apos;subordinates&apos;) &amp;&amp; ! smartlists__SOQL_Scope_My_Subordinates__c )</errorConditionFormula>
        <errorDisplayField>smartlists__SOQL_Default_Scope__c</errorDisplayField>
        <errorMessage>The default filter is not one of the selected filters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Enable_All_CRUD_Actions</fullName>
        <active>true</active>
        <errorConditionFormula>smartlists__Enable_CRUD_Actions__c &amp;&amp; ( smartlists__Enable_Delete_Record_Action__c ||  smartlists__Enable_Edit_Record_Action__c ||  smartlists__Enable_New_Record_Action__c )</errorConditionFormula>
        <errorDisplayField>smartlists__Enable_CRUD_Actions__c</errorDisplayField>
        <errorMessage>Enable All Record Actions cannot be checked when individual record actions are selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Enable_CRUD_Actions</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Files&quot;) &amp;&amp; (smartlists__Enable_CRUD_Actions__c ||  smartlists__Enable_Delete_Record_Action__c ||  smartlists__Enable_Edit_Record_Action__c ||  smartlists__Enable_New_Record_Action__c)</errorConditionFormula>
        <errorDisplayField>smartlists__Enable_CRUD_Actions__c</errorDisplayField>
        <errorMessage>Record actions cannot be enabled for Files Lists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Indirect_Relationship_FILES</fullName>
        <active>false</active>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>smartlists__Parent_Object__c</errorDisplayField>
        <errorMessage>Files datasource: Parent Object, Parent Key Field and Parent Id are required for an indirect relationship</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Indirect_Relationship_SOQL_APEX</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source&quot;) || ISPICKVAL(smartlists__Data_Source_Type__c , &quot;SOQL with Sharing&quot;) ||ISPICKVAL(smartlists__Data_Source_Type__c , &quot;SOQL without Sharing&quot;)) &amp;&amp; 
 !ISBLANK( smartlists__Parent_Key_Field__c ) &amp;&amp; ISBLANK( smartlists__Parent_Id_Field__c )</errorConditionFormula>
        <errorDisplayField>smartlists__Parent_Id_Field__c</errorDisplayField>
        <errorMessage>SOQL or APEX Data Source: Parent Key Field and Parent Id are required for an indirect relationship</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Layout_For_Tiles</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( smartlists__Display_Mode__c , &apos;Tiles&apos;) &amp;&amp;  ISPICKVAL( smartlists__Tile_Layout__c, &apos;&apos; )</errorConditionFormula>
        <errorDisplayField>smartlists__Tile_Layout__c</errorDisplayField>
        <errorMessage>Tile Layout is required when Display Mode is Tiles</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_List_Icon_APEX</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source&quot;) &amp;&amp; 
ISBLANK(  smartlists__SObject__c ) &amp;&amp; ISBLANK(   smartlists__List_Icon__c   )</errorConditionFormula>
        <errorDisplayField>smartlists__List_Icon__c</errorDisplayField>
        <errorMessage>List Icon is required for APEX Data Source without SObject</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_List_Label_APEX</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source&quot;) &amp;&amp; 
ISBLANK(  smartlists__SObject__c ) &amp;&amp; ISBLANK(  smartlists__List_Label__c  )</errorConditionFormula>
        <errorDisplayField>smartlists__List_Label__c</errorDisplayField>
        <errorMessage>List Label is required for APEX Data Source without SObject</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Parent_Field</fullName>
        <active>true</active>
        <description>Obsolete</description>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source with Parent&quot;) &amp;&amp;
 ISBLANK( smartlists__Parent_Id_Field__c )</errorConditionFormula>
        <errorDisplayField>smartlists__Parent_Id_Field__c</errorDisplayField>
        <errorMessage>Parent Id Field is required for Apex Data Source with Parent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_RowKey</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &quot;Apex Data Source&quot;) &amp;&amp;
ISBLANK( smartlists__Row_Key__c )</errorConditionFormula>
        <errorDisplayField>smartlists__Row_Key__c</errorDisplayField>
        <errorMessage>Row Key is required for type &apos; Apex Data Source&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_SObject</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(smartlists__Data_Source_Type__c , &quot;SOQL with Sharing&quot;) ||ISPICKVAL(smartlists__Data_Source_Type__c , &quot;SOQL without Sharing&quot;)) &amp;&amp;
ISBLANK(smartlists__SObject__c)</errorConditionFormula>
        <errorDisplayField>smartlists__SObject__c</errorDisplayField>
        <errorMessage>SObject is required for types &apos;SOQL with/without Sharing&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_SObject_APEX</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(smartlists__Data_Source_Type__c, &apos;Apex Data Source&apos;) &amp;&amp; ISBLANK( smartlists__SObject__c ) &amp;&amp; (smartlists__Enable_CRUD_Actions__c ||  smartlists__Enable_Delete_Record_Action__c ||  smartlists__Enable_Edit_Record_Action__c ||  smartlists__Enable_New_Record_Action__c)</errorConditionFormula>
        <errorDisplayField>smartlists__SObject__c</errorDisplayField>
        <errorMessage>SObject is required when record actions are enabled on an Apex Data Source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Show_Sosl_Search</fullName>
        <active>false</active>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>smartlists__Show_SOSL_Search_in_Quick_Filter__c</errorDisplayField>
        <errorMessage>Show SOQL Search in Quick Filter cannot be selected with the My Team records scope</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Show_Sosl_Search_Picklist</fullName>
        <active>true</active>
        <errorConditionFormula>!ISPICKVAL( smartlists__Show_SOSL_Search__c , &apos;No&apos;) &amp;&amp; smartlists__SOQL_Scope_My_Team__c</errorConditionFormula>
        <errorDisplayField>smartlists__Show_SOSL_Search__c</errorDisplayField>
        <errorMessage>Show SOQL Search in Component or Filters Panel cannot be selected with the My Team visibility filter</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>smartlists__Check_Wrap_Text_Max_Lines</fullName>
        <active>false</active>
        <errorConditionFormula>smartlists__Wrap_Text_Max_Lines__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>smartlists__Wrap_Text_Max_Lines__c</errorDisplayField>
        <errorMessage>Wrap Text Max Lines must be greater than 0</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
